{
	"version": "0.2.0",
	"configurations": [
		{
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"name": "nodemon",
			"program": "${workspaceFolder}/dist/main.js",
			"request": "launch",
			"restart": true,
			"runtimeExecutable": "nodemon",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		}
	]
}